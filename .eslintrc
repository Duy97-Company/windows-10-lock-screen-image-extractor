{
  "env": {
    "node": true,
    "browser": false,
    "es6": true
  },
  "extends": [
    "airbnb-base"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": 0,
    "quotes": [2, "single", { "avoidEscape": true }]
  }
}
